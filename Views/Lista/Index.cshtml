 @model dynamic;

  <section id="hero2" class="d-flex justify-content-center align-items-center">
    <div class="container position-relative" data-aos="zoom-in" data-aos-delay="100">
      <h1>Registro de interacciones</h1>
    </div>
  </section>
  <br>

  <br>

<div class="container">

    <table class="table" >
      <thead>
        <tr style="background-color: #910222;">
          <th style="color: white;" scope="col">ID</th>
          <th style="color: white;" scope="col">Nombre</th>
          <th style="color: white;" scope="col">Correo Institucional</th>
          <th style="color: white;" scope="col">Hora de logueo</th>
        </tr>
      </thead>
      <tbody>
        @foreach (Profesor prof in @Model.Profesores)
        {
          <tr>
            <th scope="row">@prof.ID</th>
            <td>@prof.Name</td>
            <td>@prof.Username</td>
            <td>@prof.Hora</td>
          </tr>
        }
      </tbody>
    </table>
    <br>
    <br>
    <table class="table" >
      <thead>
        <tr style="background-color: #910222;">
          <th style="color: white;" scope="col">Código Estudiante</th>
          <th style="color: white;" scope="col">Nombres</th>
          <th style="color: white;" scope="col">Apellidos</th>
          <th style="color: white;" scope="col">Hora de testeo</th>
          <th style="color: white;" scope="col">Varilla 1</th>
          <th style="color: white;" scope="col">Varilla 2</th>
        </tr>
      </thead>
      <tbody>
        @foreach (Alumno alu in @Model.Alumnos)
        {
          <tr>
            <th scope="row">@alu.ID</th>
            <td>@alu.Nombres</td>
            <td>@alu.Apellidos</td>
            <td>@alu.Hora</td>
            <td>@alu.Var1</td>
            <td>@alu.Var2</td>
          </tr>
        }
      </tbody>
    </table>
</div>